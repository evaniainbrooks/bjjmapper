<div class="team-list container">
  <h4>Filter by Team <small><a href="javascript:void(0);" data-clear-filters>clear</a></small></h4>
  <ul class="list-unstyled">
    <% _.each(teams, function(team) { %>
      <li>
        <label>
          <input data-team-id="<%= team.id %>" type="checkbox" />
          <%= team.name %>
        </label>
      </li>
    <% }); %>
  </ul>
</div>
